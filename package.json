    "registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "antd": "5.3.1",
    "classnames": "2.3.2",
    "i18next": "22.4.13",
    "is-hotkey": "0.2.0",
    "mitt": "3.0.0",
    "node-html-parser": "6.1.5",
    "react-i18next": "12.2.0",
    "react-icons": "4.8.0",
    "slate": "0.91.4",
    "slate-history": "0.86.0",
    "slate-hyperscript": "0.81.3",
    "slate-react": "0.91.8"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
