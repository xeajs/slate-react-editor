    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "father": "4.1.7",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rome": "11.0.0",
    "typescript": "4.9.5",
    "vite": "4.1.4"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run fix"
